      },
      {
        "name": "mass",
        "label": "Bob Mass",
        "min": 1,
        "max": 20,
        "default": 5,
        "step": 0.5,
        "unit": "kg"
      },
      {
        "name": "initialAngle",
        "label": "Initial Displacement",
